<table class='merchants-table'>

  <tbody>
    <% for merchant in @merchants %>
      <tr>
        <td><%= merchant.name %></td>
        <td align='right'><a href='/merchants/<%= merchant.id %>/edit'>Edit</a></td>
      </tr>
    <% end %>
  </tbody>

  <tfoot class='foot-link'>
    <tr>
      <td colspan='2' align='center'><a href='/merchants/new'>+</a></td>
    </tr>
  </tfoot>

</table>